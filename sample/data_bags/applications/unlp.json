{
"id": "unlp",
  "staging": {
    "application_servers": [
      "www.unlp.edu.ar"
    ],
    "cache" : {
      "port": 80,
      "probe": {
        "url": "/",
        "timeout": "2s",
        "interval": "6s",
        "window": 6,
        "threshold": 2
      }
    },
    "applications": {
      "frontend": {
        "listen": ["80", "443 ssl"],
        "server_name": ["www.unlp.edu.ar", "unlp.edu.ar"],
        "ssl": {
          "enabled": true
        }
      }
    }
  }
}
